<div class="row">
  <div class="col-lg-1"></div>
  <div class="col-lg-4">
    <h1>PubFinder</h1>
    <form  action="/result" method="post">
      <div class="row">
        <label for="expensiveness" class="col-lg-6">Expensiveness</label>
        <input type="range" name="expensiveness" max="5" class="col-lg-6" min="1" max="5">
      </div>
      <div class="row">
        <label for="atmosphere" class="col-lg-6">Atmosphere</label>
        <input type="range" name="atmosphere" max="5" class="col-lg-6" min="1" max="5">
      </div>
      <div class="row">
        <label for="radius" class="col-lg-6">Radius (miles)</label>
        <input type="number" name="radius" class="col-lg-6">
      </div>
        <button class="btn btn-primary btn-lg" id="search">Go!</button>
    </form>
  </div>
  <div class="col-lg-1"></div>
  <div id="map" class="col-lg-5"></div>
  <div class="col-lg-1"></div>
</div>
