<h1>Create Pub</h1>
<section>
  <div class="container newPubForm">
    <form method="POST" action="/pubs" enctype="multipart/form-data">
      <input type="hidden" name="lat">
      <input type="hidden" name="lng">
      <div>
        <label for="name">Name</label>
        <input type="text" name="name" id="name" class="form-control">
      </div>
      <div>
        <label for="address">Address</label>
        <input type="text" name="address" id="address" class="form-control autocomplete">
      </div>
      <div>
        <label for="expensiveness">Expensiveness</label>
        <select class="form-control" name="expensiveness">
          <option value="1">1</option>
          <option value="2">2</option>
          <option value="3">3</option>
          <option value="4">4</option>
          <option value="5">5</option>
        </select>
      </div>
      <div>
        <label for="atmosphere">Atmosphere</label>
        <select class="form-control" name="atmosphere">
          <option value="1">1</option>
          <option value="2">2</option>
          <option value="3">3</option>
          <option value="4">4</option>
          <option value="5">5</option>
        </select>
      </div>
      <div>
        <label for="image">Image</label>
        <input type="file" name="image" id="image">
      </div>
      <button class="btn btn-outline-primary">Add</button>
    </form>
  </div>
</section>
